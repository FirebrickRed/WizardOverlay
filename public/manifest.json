{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Wizard Overlay",
    "author": "FireBrick",
    "version": "1.0.1",
    "minimum-overwolf-version": "0.160.0",
    "description": "(WIP) Currently going to display quest line progression type stuff.",
    "dock_button_title": "Wizard Overlay",
    "icon": "icons/wizard_outfitter_logo_hover.png",
    "icon_gray": "icons/wizard_outfitter_logo.png",
    "launcher_icon": "icons/wizard_outfitter_logo.ico",
    "window_icon": "icons/wizard_outfitter_logo.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo"
  ],
  "data": {
    "start_window": "background",
    "hotkeys": {
      "sample_app_ts_showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F"
      }
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": [
        "http://*.overwolf.com",
        "https://*.overwolf.com",
        "https://*.google-analytics.com",
        "http://*.google-analytics.com",
        "https://www.googleapis.com"
      ]
    },
    "force_browser": "user",
    "windows": {
      "background": {
        "file": "background.html",
        "background_optimization": true,
        "is_background_page": true
      },
      "desktop": {
        "file": "desktop.html",
        "desktop_only": true,
        "native_window": true,
        "resizeable": true,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 950,
          "height": 300
        },
        "min_size": {
          "width": 950,
          "height": 300
        }
      },
      "in_game": {
        "file": "in_game.html",
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "sample_app_ts_showhide",
        "resizable": true,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 300,
          "height": 300
        },
        "min_size": {
          "width": 300,
          "height": 300
        }
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [4592]
    },
    "game_events": [4592],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [4592]
        },
        "start_minimized": true
      }
    ],
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
